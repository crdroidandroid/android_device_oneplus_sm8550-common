android_app {
    name: "DeviceSettings",

    srcs: ["src/**/*.java"],
    resource_dirs: ["res"],
    certificate: "platform",
    platform_apis: true,
    system_ext_specific: true,
    privileged: true,

    static_libs: [
        "SettingsLib",
        "org.lineageos.platform.internal",
    ],

    optimize: {
        proguard_flags_files: ["proguard.flags"],
    },
}
